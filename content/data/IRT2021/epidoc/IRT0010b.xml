         <div type="edition" xml:lang="la" xml:space="preserve">
            
           
            <ab>
               <lb n="1"/><persName type="divine" ref="#iuppiter"><name nymRef="Iuppiter"><expan><abbr>I</abbr><ex>oui</ex></expan></name> 
               <addName nymRef="Optimus"><expan><abbr>O</abbr><ex>ptimo</ex></expan></addName>
               <addName nymRef="Maximus"><expan><abbr>M</abbr><ex>aximo</ex></expan></addName></persName>
               <lb n="2"/><persName type="attested" key="pir-a-0334"><name type="praenomen" nymRef="Quintus"><expan><abbr>Q</abbr><ex>uintus</ex></expan></name> <name type="gentilicium" nymRef="Aemilius">Aemilius</name>
                  <lb n="3"/><name type="cognomen" nymRef="Aristides">Aristides</name></persName> 
               <lb n="4"/><w lemma="iuridicus"><expan><abbr>iurid</abbr><ex>icus</ex></expan></w> <w lemma="per">per</w> 
               <lb n="5"/><placeName nymRef="#Alexandria" ref="#alexandria"><expan><abbr><unclear>A</unclear>lexandr</abbr><ex>iam</ex></expan> </placeName>
            </ab>
