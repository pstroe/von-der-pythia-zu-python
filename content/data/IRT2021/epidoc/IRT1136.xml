                 <lb n="6" break="no"/>bus</name></persName> <w lemma="mater">matri</w>
                 <lb n="7"/><w lemma="suus"><expan><abbr>s</abbr><ex>ua</ex></expan></w> <w lemma="pecunia"><expan><abbr>p</abbr><ex>ecunia</ex></expan></w> <w lemma="facio"><expan><abbr>f</abbr><ex>ecit</ex></expan></w>
                
