                  <supplied reason="lost"><w lemma="maximus"><expan><abbr>max</abbr><ex>imo</ex></expan></w></supplied> 
                  <lb n="2"/>
                  <supplied reason="lost"><w lemma="tribunicius"><expan><abbr>trib</abbr><ex>unicia</ex></expan></w></supplied>
                     <w lemma="potestas"><expan><abbr><supplied reason="lost">p</supplied>ot</abbr><ex>estate</ex></expan></w> <num value="16">XVI</num>
                     <w lemma="imperator"><expan><abbr>i<supplied reason="lost">mp</supplied></abbr><supplied reason="lost"><ex>eratore</ex></supplied></expan></w> 
                  <supplied reason="lost"><num value="2">II</num>
